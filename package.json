{
  "name": "drow-icons",
  "author": {
    "name": "fourdusk"
  },
  "license": "Apache-2.0",
  "description": "A light icon library",
  "keywords": [
    "drow",
    "ligth",
    "icon"
  ],
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm",
    "eslint": "eslint --fix packages/**/*.{vue,js,jsx,ts,tsx}",
    "prettier": "prettier --write **/*.{vue,js,jsx,ts,tsx,json,md,css,scss,html}",
    "stylelint": "stylelint --fix src/**/*.{vue,css,scss,html}",
    "lint-staged": "lint-staged --config ./lint-staged.config.js",
    "playground": "pnpm run -C playground dev",
    "build": "pnpm -r --parallel --filter=./packages/* run build"
  },
  "dependencies": {
    "@drow-icons/svg": "workspace:^"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@iconify/tools": "^4.0.4",
    "@types/node": "^20.14.11",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "autoprefixer": "^10.4.19",
    "consola": "^3.2.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^9.27.0",
    "husky": "^9.1.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.39",
    "postcss-html": "^1.7.0",
    "postcss-load-config": "^6.0.1",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "stylelint": "^16.7.0",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "tsx": "^4.16.2",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vite-plugin-dts": "4.0.0-beta.1",
    "vue": "^3.4.31",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.0.24"
  }
}
